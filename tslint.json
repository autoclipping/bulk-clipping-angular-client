{
    "rulesDirectory": [
        "node_modules/codelyzer"
    ],
    "extends": [
        "angular-tslint-rules"
    ],
    "rules": {
        "component-selector": [
            true,
            "element",
            ["ac", "test"],
            "kebab-case"
        ],
        "curly": true,
        "template-i18n": false,
        "template-use-track-by-function": false
    }
}
